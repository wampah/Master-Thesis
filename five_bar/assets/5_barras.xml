<mujoco model="parallel_5_bar_mechanism">
    <compiler angle="degree" coordinate="local" inertiafromgeom="true"/>
    <option integrator="RK4"/>

    <asset>
        <texture name="grid" type="2d" builtin="checker" rgb1=".1 .2 .3" rgb2=".2 .3 .4" width="300" height="300"/>
        <material name="grid" texture="grid" texrepeat="8 8" reflectance=".2"/>
     </asset>

    <worldbody>
        <light name="top" pos="0 0 1"/>
        <geom size="1 1 .01" pos="0 0 -.2" type="plane" material="grid"/>
        <body name="base" pos="0 0 0">
            <geom name="fixed_link" type="box" size="0.075 0.02540 0.0508" pos="0 0 0" rgba="0.5 0.5 0.5 1"/>
            
            
            
            
            <body name="proximal1" pos="-0.075 0 0.07620" euler="0 0 -45">
                <joint name="joint1" type="hinge" axis="0 0 1" pos="0 0 0"  damping="0" stiffness="0"/>
                <geom type="box" size="0.1 0.02540 0.02540" pos="-0.1 0 0" rgba="1 0 0 1"/>
                <body name="distal1" pos="-0.2 0 -0.0508" euler="0 0 102.244623466">
                    <joint name="joint3" type="hinge" axis="0 0 1" pos="0 0 0"  damping="0" stiffness="0"/>
                    <geom type="box" size="0.2 0.02540 0.02540" pos="0.2 0 0" rgba="0 1 0 1"/>
                    <site name="end_effector" pos="0.4 0 -0.02540" size="0.025"/>
                </body>
            </body>
            <body name="proximal2" pos="0.075 0 -0.07620" euler="0 0 -135">
                <joint name="joint2" type="hinge" axis="0 0 1" pos="0 0 0"  damping="0" stiffness="0"/>
                <geom type="box" size="0.1 0.02540 0.02540" pos="-0.1 0 0" rgba="1 0 0 1"/>
                <body name="distal2" pos="-0.2 0 0.0508" euler="0 0 -102.244623466">
                    <joint name="joint4" type="hinge" axis="0 0 1" pos="0 0 0"  damping="0" stiffness="0"/>
                    <geom type="box" size="0.2 0.02540 0.02540" pos="0.2 0 0" rgba="0 1 0 1"/>
                    
                </body>
            </body>
        


        </body>
    </worldbody>
    <equality>
		<connect anchor="0.4 0 0" body1="distal1" body2="distal2" name="equality_constraint"/>
	</equality>
    <sensor>
		<framepos objtype="site" objname="end_effector"/>
		<framelinvel objtype="site" objname="end_effector"/>
	</sensor>
    <actuator>
        <motor joint="joint1" />
        <motor joint="joint2" />
    </actuator>
</mujoco>
